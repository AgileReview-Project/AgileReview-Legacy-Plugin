         locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
      <separator
            name="AgileReview.PackageExplorer.separator1"
            visible="true">
      </separator>
      <command
            commandId="de.tukl.cs.softech.agilereview.activateReviewSource"
            icon="icons/agile_icon_activated.png"
            label="Activate AgileReview Source Project"
            style="push"
            tooltip="Activate this AgileReview Source Project">
         <visibleWhen
               checkEnabled="false">
            <with
                  variable="selection">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                        <test
                              property="org.eclipse.core.resources.projectNature"
                              value="%agileReview_nature.id">
                        </test>
                     </adapt>
                  </iterate>
               </and>
            </with>
         </visibleWhen>
      </command>
      <command
            commandId="de.tukl.cs.softech.agilereview.cleanup"
            icon="icons/agile_icon.png"
            label="AgileReview Project CleanUp"
            style="push"
            tooltip="Remove AgileReview comments tags from all files of this project">
         <visibleWhen
               checkEnabled="true">
         </visibleWhen>
      </command>
      <separator
            name="AgileReview.PackageExplorer.separator2"
            visible="true">
      </separator>
